{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "lib": ["es6", "es2016", "es2016.array.include", "dom", "es2017.object"],
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "strictNullChecks": false,
      "experimentalDecorators": true,
      "outDir": "build",
      "sourceMap": true,
      "allowJs": true
    },
    "exclude": [
      "node_modules"
    ]
  }